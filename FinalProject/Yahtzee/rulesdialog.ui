   <widget class="QWidget" name="about">
    <attribute name="title">
     <string>&amp;About</string>
    </attribute>
    <widget class="QTextEdit" name="aboutDialog">
     <property name="geometry">
      <rect>
       <x>3</x>
       <y>0</y>
       <width>471</width>
       <height>281</height>
      </rect>
